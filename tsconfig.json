{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2016",
    "declaration": false,
    "module": "commonjs",
    "strictNullChecks": true,
    "sourceMap": true,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "src/test/*.spec.ts"
  ],
  "exclude": ["node_modules"]
}